{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-console": "off",
        "class-methods-use-this": "off",
        "max-len": [
            "error",
            {
                "code": 80
            }
        ]
    }
}